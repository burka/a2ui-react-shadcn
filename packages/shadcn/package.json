{
  "name": "a2ui-shadcn-ui",
  "version": "0.1.0",
  "description": "Beautiful shadcn/ui components for rendering A2UI protocol messages in React",
  "author": "burka",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/burka/a2ui-shadcn-ui.git",
    "directory": "packages/shadcn"
  },
  "homepage": "https://burka.github.io/a2ui-shadcn-ui/",
  "bugs": {
    "url": "https://github.com/burka/a2ui-shadcn-ui/issues"
  },
  "keywords": [
    "a2ui",
    "ui",
    "react",
    "shadcn",
    "shadcn-ui",
    "components",
    "protocol",
    "server-driven-ui",
    "streaming",
    "ai",
    "llm"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "dev": "tsup --watch",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "framer-motion": ">=10.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "recharts": ">=2.0.0"
  },
  "peerDependenciesMeta": {
    "recharts": {
      "optional": true
    }
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.0",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.562.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "a2ui-shadcn-ui-core": "workspace:*",
    "a2ui-shadcn-ui-react": "workspace:*",
    "framer-motion": "^12.23.26",
    "jsdom": "^25.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
